/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "typeRoots": ["node_modules/@types"],
    "paths": {
      "@angular/*": ["node_modules/@angular/*"],
      "@api": ["src/app/@api"],
      "@store": ["src/app/@store"],
    },
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
